<p><%=link_to "All Stars", "/stars"%> &nbsp <%=link_to "All Planets", "/planets"%></p>
<%= button_to "Create New Star", "/stars/new", method: :get %>
<h1>Stars</h1>
<% @stars.each do |star| %>
<h3>Name: <%=link_to "#{star.name}", "/stars/#{star.id}"%></h3> 
<p>Created at: <%=star.created_at%></p>
<%= button_to "Edit #{star.name}", "/stars/#{star.id}/edit", method: :get %>
<% end %>